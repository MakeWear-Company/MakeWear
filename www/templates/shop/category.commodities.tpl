<div class="main-container commodities">
    <div class="main-content row main-width commodities">
        <div class="tabs-wrap-category">
            <ul class="top">
                <li class="my-inset{$wearActive}" data-inset="wear">
                    <span class="tab-title">ОДЕЖДА</span>
                </li>
                <li class="my-inset{$shoesActive}" data-inset="shoes">
                    <span class="tab-title">ОБУВЬ</span>
                </li>
                <li class="my-inset{$accessoriesActive}" data-inset="accessories">
                    <span class="tab-title">АКСЕССУАРЫ</span>
                </li>
                <li class="my-inset{$brandsActive}" data-inset="brands-all">
                    <span class="tab-title">БРЕНДЫ</span>
                </li>
            </ul>
            <ul class="side">
                <li class="my-inset{$childrenActive}" data-inset="children">
                    <span class="tab-title">ДЕТИ</span>
                </li>
                <li class="my-inset{$menActive}" data-inset="men">
                    <span class="tab-title">МУЖЧИНЫ</span>
                </li>
                <li class="my-inset{$womenActive}" data-inset="women">
                    <span class="tab-title">ЖЕНЩИНы</span>
                </li>
            </ul>
            <div class="insets-wrap">
                <div class="wear-women all-insets">
                    {$wearWomen}
                </div>
                <div class="shoes-women all-insets">
                    {$shoesWomen}
                </div>
                <div class="accessories-women all-insets">
                    {$accessoriesWomen}
                </div>
                <div class="wear-men all-insets">
                    {$wearMen}
                </div>
                <div class="shoes-men all-insets">
                    {$shoesMen}
                </div>
                <div class="accessories-men all-insets">
                    {$accessoriesMen}
                </div>
                <div class="wear-children all-insets">
                    {$wearChildren}
                </div>
                <div class="shoes-children all-insets">
                    {$shoesChildren}
                </div>
                <div class="accessories-children all-insets">
                    {$accessoriesChildren}
                </div>
                <div class="brands-all all-insets">
                    {$brands}
                </div>
            </div>
        </div>
    </div>
</div>